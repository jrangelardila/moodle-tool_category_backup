<div class="d-flex gap-3">
    <form action="{{generate_url}}" method="post">
        <input type="hidden" name="execute" value="true">
        <input type="hidden" name="sesskey" value="{{sesskey}}">
        <button type="submit" class="btn btn-primary  m-2">{{#str}}generate_task, tool_category_backup{{/str}}</button>
    </form>
    <form action="{{download_url}}" method="post">
        <input type="hidden" name="download" value="true">
        <input type="hidden" name="sesskey" value="{{sesskey}}">
        <button type="submit" class="btn btn-primary  m-2">{{#str}}
            download_backups, tool_category_backup{{/str}}</button>
    </form>
    <form action="{{back_url}}" method="post">
        <input type="hidden" name="sesskey" value="{{sesskey}}">
        <button type="submit" class="btn btn-primary m-2">{{#str}}back, tool_category_backup{{/str}}</button>
    </form>
</div>



